1 Password Validation

            function checkPassword() {
              let password1;
              let password2 ;
                if (password1 == '')
                    alert ("Please enter Password")
                else if (password2 == '')
                    alert ("Please enter confirm password");
               
                else if (password1 != password2) {
                    console.log ("Password did not match. Password validation unsuccessful")
             
                }
                else{
                    console.log("Password Matched. Password validation successful")
              
                }
            }
    checkPassword();
    
    
    2Calculator
    
    
    function calculator(a,b,c){
    let output=0;
                    switch(c){
                                  case '+':
                                      output = a + b
                                      Consloe.log(output)
                                      break;
                                   case '-':
                                      output = a - b
                                      Consloe.log(output)
                                      break;
                                   case '*':
                                      output = a * b
                                      Consloe.log(output)
                                      break;
                                   case '/':
                                      output = a / b
                                      Consloe.log(output)
                                      break;
                                  default:
                                      console.log("invalid operator")
                                      break;
                    }
    }
    
    calculator(1,2,"+");
    
    
    3 color mixer
    
       function color mixer(color1,color2){
       let  output
   if((color1=="red" and color2="blue")||(color2=="red" and color1="blue"))  output=1;
   else if((color1=="red" and color2="yellow")||(color2=="red" and color1="yellow"))  output=2;
    else if((color1=="yellow" and color2="blue")||(color2=="yellow" and color1="blue"))  output=3;
                    switch(output){
                                  case 1:
                                  
                                      Consloe.log("purple")
                                      break;
                                   case 2:
                                    
                                      Consloe.log("orange")
                                      break;
                                   case 3:
                                  
                                      Consloe.log("green")
                                      break;
                                   
                                  default:
                                      console.log("invalid color combination")
                                      break;
                    }
    }
    
    color mixer(red,green)
    
